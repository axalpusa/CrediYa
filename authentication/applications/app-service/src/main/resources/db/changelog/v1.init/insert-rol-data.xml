<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.21.xsd">

    <!-- Insert ADMIN -->
    <changeSet id="4-1" author="axel" failOnError="false">
        <preConditions onFail="MARK_RAN" onError="MARK_RAN">
            <sqlCheck expectedResult="0">
                SELECT COUNT(*) FROM rol WHERE id_rol = 'facbe723-85f2-4f5a-92d6-a4a4a3a5b8ca'
            </sqlCheck>
        </preConditions>
        <insert tableName="rol">
            <column name="id_rol" value="facbe723-85f2-4f5a-92d6-a4a4a3a5b8ca"/>
            <column name="name" value="ADMIN"/>
            <column name="description" value="ADMIN"/>
        </insert>
    </changeSet>

